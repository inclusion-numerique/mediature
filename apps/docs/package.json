  "scripts": {
    "dev": "only-include-used-icons && storybook dev -p 6006 --no-open",
    "build": "only-include-used-icons && storybook build",
    "start": "serve -l 6006 storybook-static",
    "lint": "pnpm run lint:es && pnpm run lint:ts",
    "lint:es": "TIMING=1 eslint --ext .js,.jsx,.ts,.tsx,.mdx .",
    "lint:ts": "tsc --noEmit --incremental false",
    "test:coverage": "echo not working for now, see https://github.com/storybookjs/storybook/discussions/16841#discussioncomment-4605661",
    "test:coverage:backup": "start-server-and-test start http://localhost:6006 test:coverage:command",
    "test:coverage:command": "test-storybook --stories-json --coverage",
    "test:e2e": "start-server-and-test start http://localhost:6006 test:e2e:command",
    "test:e2e:command": "test-storybook --stories-json",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "chromatic": "npx chromatic --build-script-name build --auto-accept-changes --exit-zero-on-changes"
  },
