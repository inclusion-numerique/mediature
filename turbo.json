{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "heroku-postbuild": {
      "cache": false
    },
    "test:prepare": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "test:unit": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "test:unit:watch": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "test:e2e:headless": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint:prepare": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "accessibility": {
      "cache": false
    },
    "accessibility:open": {
      "cache": false
    },
    "format": {
      "cache": false
    },
    "format:check": {
      "cache": false
    },
    "chromatic": {
      "cache": false
    }
  },
  "globalEnv": [
    "APP_BASE_URL",
    "APP_MODE",
    "CI",
    "CONTAINER_VERSION",
    "CRISP_SIGNING_SECRET_KEY",
    "CRISP_WEBSITE_ID",
    "DATABASE_URL",
    "ENABLE_MOCKS",
    "FILE_AUTH_SECRET",
    "GITHUB_TOKEN",
    "JEST_MAX_WORKERS",
    "JEST_WORKER_ID",
    "MAINTENANCE_API_KEY",
    "NEXT_AUTH_SECRET",
    "NEXT_PUBLIC_APP_BASE_URL",
    "NEXT_PUBLIC_APP_MODE",
    "NEXT_PUBLIC_CRISP_WEBSITE_ID",
    "NEXT_PUBLIC_MAILER_DEFAULT_DOMAIN",
    "NEXT_PUBLIC_SENTRY_DSN",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "npm_package_version",
    "npm_package_repository_url",
    "PORT",
    "SENTRY_DSN",
    "SENTRY_RELEASE_UPLOAD",
    "SENTRY_RELEASE_TAG",
    "SENTRY_URL",
    "MAILER_DEFAULT_DOMAIN",
    "MAILER_DOMAINS_TO_CATCH",
    "MAILER_SMTP_HOST",
    "MAILER_SMTP_PORT",
    "MAILER_SMTP_USER",
    "MAILER_SMTP_PASSWORD",
    "MAILER_FALLBACK_SMTP_HOST",
    "MAILER_FALLBACK_SMTP_PORT",
    "MAILER_FALLBACK_SMTP_USER",
    "MAILER_FALLBACK_SMTP_PASSWORD",
    "MAILJET_API_KEY",
    "MAILJET_SECRET_KEY",
    "MAILJET_CLIENT_MOCK",
    "MAILJET_WEBHOOK_AUTH_USERNAME",
    "MAILJET_WEBHOOK_AUTH_PASSWORD",
    "MAILJET_WEBHOOK_DOMAIN",
    "SOURCE_VERSION",
    "STORYBOOK_ENVIRONMENT",
    "TEST_MANUAL",
    "TESTCONTAINERS_RYUK_DISABLED",
    "TIME_TO_WAIT_AFTER_CHANGING_THEME_MS",
    "TRPC_SERVER_MOCK"
  ]
}
